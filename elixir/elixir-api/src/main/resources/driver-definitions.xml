<driver-definitions>
    <driver-definition>
        <database>MySQL</database>
        <website>http://www.mysql.com</website>
        <jdbc-url>jdbc:mysql://[host]:[port]/[db]</jdbc-url>
        <driver-vendors>
            <driver-vendor>
                <name>MySQL</name>
                <website>http://www.mysql.com</website>
                <driver>
                    <class>com.mysql.jdbc.Driver</class>
                </driver>
            </driver-vendor>
            <driver-vendor>
                <name>MM-MySQL</name>
                <website>http://www.mysql.com</website>
                <driver>
                    <class>org.gjt.mm.mysql.Driver</class>
                </driver>
            </driver-vendor>
        </driver-vendors>
    </driver-definition>
