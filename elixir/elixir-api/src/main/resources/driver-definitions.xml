<drvDef:driver-definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                    xmlns:drvDef="http://www.elega9t.com/elixir/driverDefinitions">
    <drvDef:driver-definition xmlns:comn="http://www.elega9t.com/elixir/common">
        <comn:database>MySQL</comn:database>
        <comn:website>http://www.mysql.com</comn:website>
        <comn:jdbc-url>jdbc:mysql://[host]:[port]/[db]</comn:jdbc-url>
        <comn:driver-vendors>
            <comn:driver-vendor>
                <comn:name>MySQL</comn:name>
                <comn:website>http://www.mysql.com</comn:website>
                <comn:driver>
                    <comn:class>com.mysql.jdbc.Driver</comn:class>
                </comn:driver>
            </comn:driver-vendor>
            <comn:driver-vendor>
                <comn:name>MM-MySQL</comn:name>
                <comn:website>http://www.mysql.com</comn:website>
                <comn:driver>
                    <comn:class>org.gjt.mm.mysql.Driver</comn:class>
                </comn:driver>
            </comn:driver-vendor>
        </comn:driver-vendors>
    </drvDef:driver-definition>
</drvDef:driver-definitions>
